<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.CheckBox?>
<FlowPane prefHeight="400.0" prefWidth="600.0"
          xmlns="http://javafx.com/javafx/8.0.172-ea"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="pl.camp.it.controllers.SimpleController">
    <children>
        <Button mnemonicParsing="false" text="Przycisk" fx:id="przycisk1" onAction="#buttonClick"/>
        <CheckBox text="Zaznacz albo nie" fx:id="checkBox" onAction="#buttonClick" onMouseEntered="#mouseEntered"/>
        <Button text="Przycisk 2" fx:id="button2"/>
    </children>
</FlowPane>
